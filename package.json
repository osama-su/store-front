{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "start": "tsc && node build/index.js",
    "watch": "tsc-watch --esModuleInterop src/index.ts --outDir ./build --onSuccess \"node ./build/index.js\"",
    "migrate": "db-migrate up -m ./src/database/migrations",
    "test": "export NODE_ENV=test &&  db-migrate up -m ./src/database/migrations --env test && tsc && jasmine && db-migrate down -m ./src/database/migrations --env test",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix"
  },
  "author": "Osama Ahmed",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
